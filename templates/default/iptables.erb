#

<% unless @iptables.nil? || @iptables.empty? -%>
<% @iptables.each do |table, data| -%>
*<%= table %>
<% unless data['chains'].nil? || data['chains'].empty? -%>
<% data['chains'].each do |chain, chain_data| -%>
:<%= chain %> <%= chain_data[:value] %>
<% unless chain_data['rules'].nil? || chain_data['rules'].empty? -%>
<% chain_data['rules'].each do |rule| -%>
<%= rule %>
<% end -%>
<% end -%>
<% end -%>
<% end -%>
COMMIT
<% end -%>
<% end -%>
